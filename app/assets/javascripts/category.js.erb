$(document).on("turbolinks:load", function(){
    <% Category.all.each do |category| %>
      $('#category<%=category.id %>').click(function(){
        $('.selected').removeClass('selected');
        $(this).addClass('selected');
        $('#qas div:not(.category<%=category.id %>).qa').hide();
        $('#qas .category<%=category.id %>').show();

      //インクリメンタルサーチを再実行
      var categoryId = $('.selected')[0].id;
      $('.qa').hide();
      $('.qa.' + categoryId + ':contains(' + $('#inc_search')[0].value + ')').show();
      }),
    <% end %>
    function(){}
})
